---
import ExperienceItem from './ExperienceItem.astro'
import { EXPERIENCE } from '../core/constants'
---

<ol class="relative ml-1.5 space-y-10 border-s border-gray-400 pb-10">
	{
		EXPERIENCE.map((experience) => (
			<li class="ms-8">
				<ExperienceItem {...experience} />
			</li>
		))
	}
</ol>
